{
	"name": "game.local",
	"version": "1.0.0",
	"description": "A game project",
	"main": "index.js",
	"scripts": {
		"start": "parcel _dev/index.html --out-dir $npm_config_public --open",
		"mk": "make-dir $npm_config_public",
		"clean": "del --force $npm_config_public",
		"parcelDev": "parcel build _dev/index.html --out-dir $npm_config_public",
		"dev": "NODE_ENV=dev run-s clean mk parcelDev postRun",
		"parcelProd": "parcel build --no-source-maps _dev/index.html --out-dir $npm_config_public",
		"prod": "NODE_ENV=production run-s clean mk parcelProd postRun",
		"postRun": "ncp admin $npm_config_public/admin/ && ncp api $npm_config_public/api/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LeFodeurCou/game-project.git"
	},
	"author": "Le Fodeur Cou",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/LeFodeurCou/game-project/issues"
	},
	"homepage": "https://github.com/LeFodeurCou/game-project#readme",
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"del-cli": "^4.0.1",
		"make-dir-cli": "^3.0.0",
		"move-cli": "^2.0.0",
		"ncp": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.5.0",
		"sass": "^1.51.0"
	},
	"dependencies": {
		"react": "^18.1.0",
		"react-dom": "^18.1.0"
	}
}
